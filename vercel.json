{
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "app.py" },
    { "src": "/(.*)", "dest": "/static/index.html" }
  ],
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/app.py" },
    { "source": "/(.*)", "destination": "/static/index.html" }
  ]
}
