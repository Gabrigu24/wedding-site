{
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/(.*)", "destination": "/public/$1" }
  ],
  "version": 2,
  "builds": [
    { "src": "api/gifts.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "api/gifts.js" }
  ],
  "functions": {
    "api/gifts.js": {
      "maxDuration": 10
    }
  }
}
